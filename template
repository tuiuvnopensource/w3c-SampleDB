create new Db with super_User in phpmyadmin (xampp)

CREATE USER 'groot'@'localhost' IDENTIFIED VIA mysql_native_password USING '***';GRANT ALL PRIVILEGES ON *.* TO 'groot'@'localhost' REQUIRE NONE WITH GRANT OPTION MAX_QUERIES_PER_HOUR 0 MAX_CONNECTIONS_PER_HOUR 0 MAX_UPDATES_PER_HOUR 0 MAX_USER_CONNECTIONS 0;CREATE DATABASE IF NOT EXISTS `groot`;GRANT ALL PRIVILEGES ON `groot`.* TO 'groot'@'localhost';GRANT ALL PRIVILEGES ON `groot\_%`.* TO 'groot'@'localhost';
--
